{
    "restartable": "rs",
    "ignore": [
        ".git",
        "node_modules/",
        "dist/app",
        "dist/lib",
        "coverage/",
        "LICENSE",
        "package.json",
        "package-lock.json",
        "README.md"
    ],
    "watch": ["./"],
    "execMap": {
        "ts": "node -r ts-node/register"
    },
    "env": {
        "NODE_ENV": "development"
    },
    "ext": "js,json,ts"
}
